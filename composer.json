{
	"name": "aipng/open-graph",
	"description": "Simple library for generating basic Open Graph tags",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Karel Horsky",
			"email": "karel.horsky@centrum.cz"
		}
	],
	"autoload": {
		"psr-4": {
			"AipNg\\OpenGraph\\": "src/",
			"AipNg\\OpenGraphTests\\": "tests/src/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"require": {
		"php": ">= 8.1",
		"aipng/value-objects": "^3.0.0",
		"myclabs/php-enum": "^1.8",
		"nette/utils": "^3.0 || ^4.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.10.3",
		"phpstan/phpstan-phpunit": "^1.3.10",
		"phpunit/phpunit": "^10.0.14"
	}
}
