{
	"name": "aipng/open-graph",
	"description": "Simple library for generating basic Open Graph tags",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Karel Horsky",
			"email": "karel.horsky@centrum.cz"
		}
	],
	"autoload": {
		"psr-4": {
			"AipNg\\OpenGraph\\": "src/",
			"AipNg\\OpenGraphTests\\": "tests/src/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"require": {
		"php": ">= 7.4",
		"aipng/value-objects": "^2.1",
		"myclabs/php-enum": "^1.8",
		"nette/utils": "^2.5 || ^3.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.17",
		"phpstan/phpstan-phpunit": "^0.12.6",
		"phpunit/phpunit": "^9.0"
	}
}
